{
  "name": "@gdknot/form_validator",
  "version": "0.2.1",
  "description": "form validator for common frontend development",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:watch": "concurrently --kill-others \"tsc --project tsconfig.build.json && tsc-alias -w -p tsconfig.build.json\"",
    "serve": " VITE_APP_ENV=default vite",
    "prettier": "prettier --write 'src/**/*.vue' 'src/**/*.ts'",
    "test": "clear && jest",
    "test:watch": "clear && jest --watch",
    "---------- DOC ------------": "",
    "storybook": "start-storybook",
    "deploy:storybook": "gh-pages -d storybook-static",
    "build:storybook": "build-storybook && serve storybook-static",
    "doc": "merge-markdown -m merge_markdown.yml && typedoc src/index.ts --watch",
    "serve:doc": "merge-markdown -m merge_markdown.yml && typedoc src/index.ts && serve docs/build",
    "---------- NPM ------------": "",
    "login": "npm login",
    "publish": "npm publish",
    "---------- INSTALL ---------": "",
    "install:node": "nvm install v14.18.0 && nvm alias default v14.18.0",
    "install:storybook": "npx storybook init",
    "config:vscode": "git config --global core.editor 'code --wait'"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "files": [
    "dist",
    "tsconfig.json"
  ],
  "main": "dist/src/index.ts",
  "types": "dist/src/index.d.ts",
